{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "Pester": {
            "type": "object",
            "properties": {
                "TestResult": {
                    "type": "object",
                    "properties": {
                        "Enabled": {
                            "type": "boolean"
                        }
                    },
                    "required": [
                        "Enabled"
                    ]
                },
                "Output": {
                    "type": "object",
                    "properties": {
                        "Verbosity": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "Verbosity"
                    ]
                }
            },
            "required": [
                "TestResult",
                "Output"
            ]
        }
    }
}